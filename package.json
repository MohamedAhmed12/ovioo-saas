    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^14.17.0",
    "typeorm": "^0.3.17"
